{
  "name": "typeofit",
  "version": "0.0.6",
  "description": "A tiny composable type guard utility library",
  "type": "module",
  "main": "lib/cjs/index.js",
  "typings": "lib/ts/index.d.ts",
  "module": "lib/esm/index.mjs",
  "author": "Dan Beaven <dm.beaven@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pingid/typeofit.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/ts/index.d.ts",
      "require": "./lib/cjs/index.js",
      "module": "./lib/esm/index.js"
    }
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*",
    "tsconfig.json"
  ],
  "keywords": [
    "validation",
    "typescript",
    "inference",
    "guard",
    "type"
  ],
  "scripts": {
    "build": "npm run esm && npm run cjs",
    "esm": "tsc --module nodenext --outDir lib/esm",
    "cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
